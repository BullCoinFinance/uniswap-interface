{
   catalogs: [{
      path: "<rootDir>/src/locales/{locale}/messages",
      include: ["<rootDir>"],
      exclude: ["**/node_modules/**", "**/build/**"],
   }],
   compileNamespace: 'cjs',
   extractBabelOptions: {},
   fallbackLocales: {},
   format: po,
   locales: ["en", "it"],
   orderBy: 'messageId',
   pseudoLocale: '',
   rootDir: '.',
   runtimeConfigModule: ['@lingui/core', 'i18n'],
   sourceLocale: "en",
}