{
   catalogs: [{
      path: '<rootDir>/src/locales/{locale}/messages',
      include: ['<rootDir>'],
      exclude: ['**/node_modules/**', '**/build/**'],
   }],
   compileNamespace: 'cjs',
   extractBabelOptions: {},
   fallbackLocales: {},
   format: po,
   locales: [
      'en',
      'de',
      'es-AR',
      'es-US',
      'it-IT',
      'iw',
      'ro',
      'ru',
      'vi',
      'zh-CN',
      'zh-TW'
   ],
   orderBy: 'messageId',
   pseudoLocale: '',
   rootDir: '.',
   runtimeConfigModule: ['@lingui/core', 'i18n'],
   sourceLocale: 'en',
}